<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Book Collection</title>
  </head>
  <body>
    <main>
        <% searchResults.forEach( book => { %>
            <img src="<%= book.image %>" alt="<% book.title %>"/>
            <h3> <%= book.title %></h3>
            <h3> <%= book.author %></h3>
            <h3> <%= book.description %></h3>
        <% }) %>
    </main>
  </body>
</html>